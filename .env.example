# Primary Gmail address for Gmail and Google Calendar polling.
GOG_ACCOUNT_1=your-primary@gmail.com

# Secondary Gmail address for Gmail and Google Calendar polling.
GOG_ACCOUNT_2=your-secondary@gmail.com

# Private Discord channel ID for briefings, digests, alerts, and Q&A.
DISCORD_CHANNEL=your-discord-channel-id
